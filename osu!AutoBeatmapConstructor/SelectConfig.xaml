<Window x:Class="osu_AutoBeatmapConstructor.SelectConfig"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:osu_AutoBeatmapConstructor"
        mc:Ignorable="d"
        Title="Select config" Height="245" Width="155" ResizeMode="CanMinimize">
    <Grid>
        <ListView x:Name="configsListView" HorizontalAlignment="Left" Height="169" Margin="10,11,0,0" VerticalAlignment="Top" Width="129" ItemsSource="{Binding Path=Patterns,Mode=TwoWay}"/>
        <Button x:Name="button" Content="Select" HorizontalAlignment="Left" Margin="76,185,0,0" VerticalAlignment="Top" Width="63" Click="button_Click"/>
        <Button x:Name="cancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="10,185,0,0" VerticalAlignment="Top" Width="61" Click="cancelButton_Click"/>
    </Grid>
</Window>
